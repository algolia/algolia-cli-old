{
  "name": "@algolia/cli",
  "version": "3.1.7",
  "description": "A Node CLI tools for manipulating data. Handy for day-to-day Algolia SE work.",
  "license": "ISC",
  "author": "Algolia, Inc. (https://www.algolia.com)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/algolia/algolia-cli.git"
  },
  "bin": {
    "algolia": "./index.js"
  },
  "scripts": {
    "test": "jest --runInBand",
    "test:unit": "jest scripts/",
    "test:unit:watch": "jest --watch scripts/",
    "test:integration": "jest --runInBand tests/integration/",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "engines": {
    "node": ">=8.9.1",
    "yarn": ">=1.10.1"
  },
  "renovate": {
    "extends": [
      "config:js-app"
    ]
  },
  "keywords": [
    "data",
    "json",
    "csv",
    "manipulate",
    "transform",
    "process",
    "parse",
    "import",
    "index",
    "solutions",
    "se",
    "cli"
  ],
  "dependencies": {
    "JSONStream": "^1.3.5",
    "agentkeepalive": "^4.0.0",
    "algoliasearch": "^3.31.0",
    "async": "^2.6.0",
    "batch-stream": "^0.1.3",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "csvtojson": "^2.0.8",
    "event-stream": "3.3.4",
    "stream-transform": "^1.0.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.9.0",
    "eslint-config-algolia": "^13.2.3",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "striptags": "^3.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "testURL": "http://localhost/",
    "setupFiles": [
      "./tests/config.js"
    ]
  }
}
